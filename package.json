    "grunt-contrib-uglify": "~0.7",
    "grunt-ng-annotate": "~0.8"
  },
  "jspm": {
    "name": "moonridge",
    "main": "socket.io-rpc-client-angular.js",
    "configFile": "dist\\build-config.js",
